{
  "name": "@chat-sdk-expo/db",
  "version": "0.1.0",
  "description": "Database types and interfaces for chat applications",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "database",
    "chat",
    "types",
    "ai-sdk"
  ],
  "author": "Dustin Doan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dustindoan/chat-sdk-expo.git",
    "directory": "packages/db"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "tsup": "^8.4.0",
    "typescript": "~5.9.2"
  }
}
